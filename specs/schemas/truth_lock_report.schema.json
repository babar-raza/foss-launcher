{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "truth_lock_report.schema.json",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "ok",
    "pages",
    "unresolved_claim_ids",
    "issues"
  ],
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "ok": {
      "type": "boolean"
    },
    "pages": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "path",
          "claim_ids"
        ],
        "properties": {
          "path": {
            "type": "string"
          },
          "claim_ids": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "unresolved_claim_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "inferred_claim_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "forbidden_inferred_claim_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "issues": {
      "type": "array",
      "items": {
        "$ref": "issue.schema.json"
      }
    }
  }
}
